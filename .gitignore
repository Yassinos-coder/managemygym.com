# Ignore node_modules in both frontend and backend
/frontend/node_modules
/backend/node_modules

# Ignore build files for React in frontend
/frontend/build
/frontend/.cache

# Ignore logs in backend
/backend/logs
/backend/*.log
/backend/logs/*.log

# Ignore environment variables
/frontend/.env
/backend/.env

# Ignore coverage directory for testing
/frontend/coverage
/backend/coverage

# Ignore npm debug files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore lock files
/frontend/package-lock.json
/backend/package-lock.json
/frontend/yarn.lock
/backend/yarn.lock

# Ignore miscellaneous IDE or editor files
.vscode/
.idea/
*.swp

# Ignore uploads or other media files stored in backend
/backend/uploads

# Ignore temporary files
*.tmp
